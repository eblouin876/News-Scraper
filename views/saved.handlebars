<div class="container">

    <div class="row">
        <div class="col s10 offset-s1">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text"><span class="card-title center">Saved Articles</span>
                    <div class="card-action center"><a href="/">Add more articles</a><a href="#">Clear saved</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <ul class="collapsible">
            {{#each articles}}
            {{> article}}
            {{/each}}
        </ul>
    </div>

</div>